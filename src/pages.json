{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "media",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#1677ff",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/shouye.png",
        "selectedIconPath": "static/tabbar/shouye-select.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/yinyue.png",
        "selectedIconPath": "static/tabbar/yinyue-select.png",
        "pagePath": "pages/music/index",
        "text": "音乐"
      },
      {
        "iconPath": "static/tabbar/shipin.png",
        "selectedIconPath": "static/tabbar/shipin-select.png",
        "pagePath": "pages/video/index",
        "text": "视频"
      },
      {
        "iconPath": "static/tabbar/wode.png",
        "selectedIconPath": "static/tabbar/wode-select.png",
        "pagePath": "pages/my/index",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/music/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "音乐"
      }
    },
    {
      "path": "pages/video/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "视频"
      }
    },
    {
      "path": "pages/my/index",
      "type": "page",
      "style": {
        "navigationStyle": "default",
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/login/index",
      "type": "page",
      "style": {
        "navigationStyle": "default",
        "navigationBarTitleText": "登录"
      }
